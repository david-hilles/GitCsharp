<Window x:Class="WpfAppEntity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppEntity"
        xmlns:y="clr-namespace:WpfAppEntity.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <y:ucLogin x:Name="ucLoginForm"
            sucess="UcLogin_sucess"
                   fail="UcLogin_fail" Margin="0,0,-0.4,0"></y:ucLogin>
        <y:ucGridView x:Name="ucGridView" Visibility="Hidden"></y:ucGridView>
        
    </Grid>
</Window>
